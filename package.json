{
  "name": "n8n-nodes-crowdstrike",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for CrowdStrike Falcon providing 12 resources and 80+ operations for endpoint security, threat detection, incident response, and vulnerability management.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-crowdstrike.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-crowdstrike#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-crowdstrike/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "crowdstrike",
    "falcon",
    "edr",
    "endpoint-detection",
    "threat-intelligence",
    "incident-response",
    "security",
    "soar",
    "vulnerability-management"
  ],
  "main": "dist/nodes/CrowdStrike/CrowdStrike.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CrowdStrikeOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CrowdStrike/CrowdStrike.node.js",
      "dist/nodes/CrowdStrike/CrowdStrikeTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.35.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
